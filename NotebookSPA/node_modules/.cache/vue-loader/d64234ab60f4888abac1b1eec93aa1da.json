talert(error.request);\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\talert(error);\r\n\t\t\t\t\t\tconsole.log(error.message);\r\n\t\t\t\t\t\tconsole.log(error.config);\r\n\t\t\t\t\t});\r\n\t\t\t\t/*\r\n\t\t\t\t$.ajax({\r\n\t\t\t\t\turl: \"http://localhost:8080/NotebookSPA0/servlet/UserSigninServlet\",\r\n\t\t\t\t\tdata: self.user,\r\n\t\t\t\t\tdataType: \"text\",\r\n\t\t\t\t\tsuccess: function(res) {\r\n\t\t\t\t\t\talert(res);\r\n\t\t\t\t\t\tif (res > 0) {\r\n\t\t\t\t\t\t\tself.$router.push({\r\n\t\t\t\t\t\t\t\tpath: \"/item_list\",\r\n\t\t\t\t\t\t\t\tparams: {\r\n\t\t\t\t\t\t\t\t\tuserid: data\r\n\t\t\t\t\t\t\t\t},\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t},\r\n\t\t\t\t\terror: function() {\r\n\t\t\t\t\t\talert(\"network error!\");\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t\t*/\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped>\r\n\tdiv.TheSignup div {\r\n\t\tmargin: 20px auto;\r\n\t}\r\n.alert-danger p{\r\n\theight:60px;\r\n\tline-height: 60px;\r\n\tfont-weight: bolder;\r\n}\r\n\tlabel {\r\n\t\tmargin-right: 20px;\r\n\t}\r\n</style>\n"]}]}